    "type": "object",
    "properties": {
        "resourceGroupName": {
            "required": true,
            "type": "string"
        },
        "serviceName": {
            "required": true,
            "type": "string",
            "minLength": 1,
            "maxLength": 50,
            "pattern": "^[a-zA-Z](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?$"
        },
        "subscriptionId": {
            "required": true,
            "type": "string"
        },
        "api_version": {
            "required": true,
            "type": "string"
        },
        "_filter": {
            "required": false,
            "type": "string"
        },
        "_top": {
            "required": false,
            "type": "integer",
            "format": "int32",
            "minimum": 1
        },
        "_skip": {
            "required": false,
            "type": "integer",
            "format": "int32",
            "minimum": 0
        }
    }
}